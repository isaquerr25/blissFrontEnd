{
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": ["prettier"],
  "extends": ["airbnb", "prettier", "plugin:react/jsx-runtime"],
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "global-require": "off",
    "import/prefer-default-export": "off",
    "no-unused-vars": "warn",
    "multiline": true
  }
}
